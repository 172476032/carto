<% _(symbolizers).each(function(symbolizer, name) { %>
\section{<%= name %>}
  <% _(symbolizer).chain()
      .filter(function(p) { return p.css; })
      .each(function(p, name) {
  %>
  \subsection{<%= p.css %>}

  \starttext
  <%= p.doc %>
  \stoptext
  \startdescr{Value Type} <% if (_.isArray(p.type)) { %><%= p.type.join(', ') %><% } else { %><%= p.type %><% } %>
  \stopdescr
  <% if (typeof p['default-value'] !== 'undefined') { %>
  \startdescr{Default Value} <%= ('' + p['default-value']).replace('#', '\#') %>
  \stopdescr
  <% } %>
  <% if (p['default-meaning']) { %>
  \startdescr{Default Value Meaning} <%= p['default-meaning'] %>
  \stopdescr
  <% } %>
<% }); %>
<% }); %>
