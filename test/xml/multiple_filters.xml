  <Style name="id-world-1">
    <Rule>
      
      <PolygonSymbolizer fill="#eeeeee"/>
      <LineSymbolizer stroke="#cccccc"
       stroke-width="0.5"/>
    </Rule>
  </Style>

  <Style name="id-world-2">
    <Rule>
      <Filter>[POP2005] &gt; 1000</Filter>
      <PolygonSymbolizer fill="#aaaaaa"/>
    </Rule>
  </Style>

  <Style name="id-world-3">
    <Rule>
      <Filter>[POP2005] &lt; 1000</Filter>
      <PolygonSymbolizer fill="#bbbbbb"/>
    </Rule>
  </Style>
